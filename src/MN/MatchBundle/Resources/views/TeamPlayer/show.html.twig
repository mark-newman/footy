{% extends 'MNUsefulBundle::admin_layout.html.twig' %}

{% block content -%}
    <h1>TeamPlayer</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Paid</th>
                <td>{{ entity.paid }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('admin_teamplayer') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_teamplayer_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
